{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "create-block/get-started",
  "version": "0.1.0",
  "title": "Get started",
  "category": "widgets",
  "icon": "layout",
  "description": "Get started for subscribe and cta.",
  "supports": {
    "html": false
  },
  "attributes": {
    "title": {
      "type": "string",
      "selector": "h1"
    },
    "text": {
      "type": "string",
      "selector": "p"
    },
    "url": {
      "type": "string",
      "source": "attribute",
      "attribute": "src",
      "selector": "img"
    },
    "alignment": {
      "type": "string",
      "default": "left"
    }
  },
  "textdomain": "get-started",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css"
}